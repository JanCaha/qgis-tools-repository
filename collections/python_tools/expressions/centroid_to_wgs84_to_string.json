{
    "author": "Jan Caha",
    "exported_at": "2022-03-22T17:02:25",
    "expressions": [
        {
            "description": "Converts geometry to centroid with CRS WGS-84, then extract coordinates as a string in degrees, minutes.",
            "expression": "with_variable('point', transform(centroid( $geometry ), @layer_cr, 'EPSG:4326'), concat(regexp_replace(to_dms(x(@point), 'x', 5, 'suffix'), ',', '.'), ' ', regexp_replace(to_dms(y(@point), 'y', 5, 'suffix'), ',', '.')))",
            "group": "user",
            "name": "centroid_to_wgs84_to_string",
            "type": "expression"
        }
    ],
    "qgis_version": "3.24.1-Tisler"
}